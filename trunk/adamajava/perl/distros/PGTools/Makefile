	  lib/File/Path.pm $(INST_MAN3DIR)/File::Path.$(MAN3EXT) \
	  lib/PGTools/Annotate.pm $(INST_MAN3DIR)/PGTools::Annotate.$(MAN3EXT) \
	  lib/PGTools/Help.pm $(INST_MAN3DIR)/PGTools::Help.$(MAN3EXT) \
	  lib/Mojo/Util.pm $(INST_MAN3DIR)/Mojo::Util.$(MAN3EXT) \
	  lib/PGTools/Group.pm $(INST_MAN3DIR)/PGTools::Group.$(MAN3EXT) \
	  lib/PGTools/Merge.pm $(INST_MAN3DIR)/PGTools::Merge.$(MAN3EXT) \
	  lib/PGTools/Util/FDR.pm $(INST_MAN3DIR)/PGTools::Util::FDR.$(MAN3EXT) \
	  lib/PGTools/Configuration.pm $(INST_MAN3DIR)/PGTools::Configuration.$(MAN3EXT) 




